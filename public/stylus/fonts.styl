    src:    url('../fonts/parqueadero/fonts/parqueadero.eot?9c09pg#iefix') format('embedded-opentype'),
        url('../fonts/parqueadero/fonts/parqueadero.ttf?9c09pg') format('truetype'),
        url('../fonts/parqueadero/fonts/parqueadero.woff?9c09pg') format('woff'),
        url('../fonts/parqueadero/fonts/parqueadero.svg?9c09pg#parqueadero') format('svg');
    font-weight: normal;
    font-style: normal;
}

.icons {
    /* use !important to prevent issues with browser extensions that change fonts */
